	<DataSources>
		<DataSource Name="Reels">
			<ConnectionProperties>
				<ConnectString>data source=..\\..\\..\\..\\..\\..\\..\\Data\\Reels.db;</ConnectString>
				<DataProvider>SQLITE</DataProvider>
			</ConnectionProperties>
		</DataSource>
	</DataSources>
	<DataSets>
		<DataSet Name="SalesData">
			<Fields>
				<Field Name="SaleDate">
					<DataField>MonthYear</DataField>
				</Field>
				<Field Name="Quantity">
					<DataField>Quantity</DataField>
				</Field>
				<Field Name="Profit">
					<DataField>Profit</DataField>
				</Field>
				<Field Name="Month">
					<DataField>Month</DataField>
				</Field>
				<Field Name="Year">
					<DataField>Year</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>SELECT Sale.SalesID, Sale.SaleDate, strftime('%m',Sale.SaleDate) AS [Month], strftime('%Y',Sale.SaleDate) AS [Year], SaleDetails.Quantity, SaleDetails.Price, SaleDetails.Discount, Product.StorePrice, (((1-SaleDetails.Discount)*SaleDetails.Price)-Product.StorePrice)*SaleDetails.Quantity AS Profit
FROM Product INNER JOIN (Sale INNER JOIN SaleDetails ON Sale.SalesID=SaleDetails.SaleID) ON Product.ProductID=SaleDetails.ProductID
WHERE ((((strftime('%Y',Sale.SaleDate) > strftime('%Y',@StartDate)) Or (strftime('%m',Sale.SaleDate)>=strftime('%m',@StartDate) And strftime('%Y',Sale.SaleDate)=strftime('%Y',@StartDate))) Or @StartDate Is Null) And (((strftime('%Y',Sale.SaleDate)&lt; strftime('%Y',@EndDate)) Or (strftime('%m',Sale.SaleDate) &lt;= strftime('%m',@EndDate) And strftime('%Y',Sale.SaleDate)=strftime('%Y',@EndDate))) Or @EndDate Is Null));</CommandText>
				<DataSourceName>Reels</DataSourceName>
				<QueryParameters>
					<QueryParameter Name="@StartDate">
						<Value>=Parameters!StartDate.Value</Value>
					</QueryParameter>
					<QueryParameter Name="@EndDate">
						<Value>=Parameters!EndDate.Value</Value>
					</QueryParameter>
				</QueryParameters>
			</Query>
		</DataSet>
		<DataSet Name="DateRanges">
			<Fields>
				<Field Name="MonthYear">
					<DataField>MonthYear</DataField>
				</Field>
				<Field Name="Month">
					<DataField>Month</DataField>
				</Field>
				<Field Name="Year">
					<DataField>Year</DataField>
				</Field>
				<Field Name="Label">
					<DataField>Label</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>SELECT DISTINCT strftime('%Y-%m-01',Sale.SaleDate) AS MonthYear, strftime('%m',Sale.SaleDate) AS [Month], strftime('%Y',SaleDate) AS [Year], strftime('%m %Y', Sale.SaleDate) AS Label
FROM Sale
ORDER BY strftime('%Y',SaleDate), strftime('%m',Sale.SaleDate)</CommandText>
				<DataSourceName>Reels</DataSourceName>
			</Query>
		</DataSet>
	</DataSets>
