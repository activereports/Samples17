	<DataSets>
		<DataSet Name="Performers">
			<Fields>
				<Field Name="MoviePersonID">
					<DataField>MoviePersonID</DataField>
				</Field>
				<Field Name="FirstName">
					<DataField>FirstName</DataField>
				</Field>
				<Field Name="LastName">
					<DataField>LastName</DataField>
				</Field>
				<Field Name="CrewTitleID">
					<DataField>CrewTitleID</DataField>
				</Field>
				<Field Name="MovieID">
					<DataField>MovieID</DataField>
				</Field>
				<Field Name="Title">
					<DataField>Title</DataField>
				</Field>
				<Field Name="YearReleased">
					<DataField>YearReleased</DataField>
				</Field>
				<Field Name="Quantity">
					<DataField>Quantity</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>SELECT MoviePerson.MoviePersonID, MoviePerson.FirstName, MoviePerson.LastName, CrewTitle.CrewTitleID, Movie.MovieID, Movie.Title, Movie.YearReleased, Sum(SaleDetails.Quantity) AS Quantity
FROM (Product INNER JOIN SaleDetails ON Product.ProductID = SaleDetails.ProductID) INNER JOIN ((CrewTitle INNER JOIN (Movie INNER JOIN (MoviePerson INNER JOIN MovieCrew ON MoviePerson.MoviePersonID = MovieCrew.CastID) ON Movie.MovieID = MovieCrew.MovieID) ON CrewTitle.CrewTitleID = MovieCrew.TitleID) INNER JOIN MovieProduct ON Movie.MovieID = MovieProduct.MovieID) ON Product.ProductID = MovieProduct.ProductID
WHERE (((CrewTitle.CrewTitleID)=1))
GROUP BY MoviePerson.MoviePersonID, MoviePerson.FirstName, MoviePerson.LastName, CrewTitle.CrewTitleID, Movie.MovieID, Movie.Title, Movie.YearReleased
ORDER BY MoviePerson.LastName</CommandText>
				<DataSourceName>Reels</DataSourceName>
			</Query>
		</DataSet>
	</DataSets>
